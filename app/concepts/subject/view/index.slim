.row
  .col-sm-12 class="user"
    h3 Subject database
hr style="margin-top: 0px;"

.row style="margin: 20px;"
  table id="subjects" class="display" width="100%"

javascript:


  $(document).ready(function() {
    $('#subjects').DataTable( {
        data: [#{subject_array}],
        columns: [
            { title: "Firstname" },
            { title: "Lastname" },
            { title: "Date of Birth" },
            { title: "Height" },
            { title: "Weight." }
        ]
    } );
  });
