body
  . style="width:75%;"
      canvas id="canvas" style:"-moz-user-select: none; -webkit-user-select: none;-ms-user-select: none;"
  br
  br
  button id="randomizeData" Randomize Data
  button id="addData" Add Data
  button id="removeData" Remove Data

javascript:
    function getTimeString(time) {
      return moment(time, 'mm:ss').format();
    }

    function getDataSet(time, yArray) {
      var array_size = yArray.length - 1;
      var data = new Array();
      for(var i=0;i < array_size; i++) {
        var obj = {
                    x: getTimeString(time[i]),
                    y: yArray[i]
                  };
        data.push(obj);
      }
      return data;
    }

  var ctx = document.getElementById("canvas").getContext("2d");
  var scatterChart = new Chart(ctx, {
                                      type: 'line',
                                      data: {
                                          datasets: [{
                                              label: 'Scatter Dataset',
                                              data: getDataSet(#{time}, #{vo2})
                                          }],
                                      },
                                      options: {
                                          scales: {
                                              xAxes: [{
                                                  type: 'time',
                                                  position: 'bottom'
                                              }]
                                          }
                                      }
                                      });
